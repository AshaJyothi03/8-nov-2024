#Function to Find Prime Numbers in a Range
def prime_range(n1,n2):

    print("prime numbers from", n1, "to ",n2)
    for num in range(n1,n2+1):
        if num>1:
            for i in range(2,num):
                if (num%i)==0:
                    break

            else:
                print(num,end=" ")

prime_range(6, 30)


